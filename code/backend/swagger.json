  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "LoginRequest": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "example": "securePassword"
          },
          "userName": {
            "type": "string",
            "example": "example@example.com"
          }
        }
      },

      "LoginRequestManager": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string"
          },
          "userName": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          }
        },
        "example": {
          "password": "securePassword",
          "userName": "example@example.com",
          "teamId": "exampleTeamId"
        }
      },

      "TeamLoginRequest": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          }
        },
        "example": {
          "password": "securePassword",
          "email": "example@example.com",
          "firstName": "John",
          "lastName": "Doe",
          "teamId": "exampleTeamId",
          "teamName": "TeamName"
        }
      },

      "SessionRequest": {
        "type": "object",
        "properties": {
          "sessionId": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          },
          "sessionName": {
            "type": "string"
          },
          "createdAt": {
            "type": "integer"
          },
          "updatedAt": {
            "type": "integer"
          },
          "impactHistory": {
            "type": "string"
          }
        },
        "example": {
          "sessionId": "sessionId",
          "sessionName": "sessionName",
          "createdAt": 486486,
          "updatedAt": 486486,
          "teamId": "exampleTeamId",
          "impactHistory": {
            "12": [
              {
                "magntitude": 10,
                "direction": "left",
                "timestamp": 123456789,
                "isConcussion": true
              }
            ],
            "13": [
              {
                "magntitude": 10,
                "direction": "left",
                "timestamp": 123456789,
                "isConcussion": false
              }
            ]
          }
        }
      },

      "HubRequest": {
        "type": "object",
        "properties": {
          "hubId": {
            "type": "string"
          },
          "hubKey": {
            "type": "string"
          }
        },
        "example": {
          "hubId": "exampleHubId",
          "hubKey": "exampleHubKey"
        }
      },

      "HubResponse": {
        "type": "object",
        "properties": {
          "mqttUsername": {
            "type": "string"
          },
          "mqttPassword": {
            "type": "string"
          },
          "mqttCA": {
            "type": "string"
          }
        },
        "example": {
          "mqttUsername": "exampleUsername",
          "mqttPassword": "examplePassword",
          "mqttCA": "exampleCA"
        }
      },

      "Hub": {
        "type": "object",
        "properties": {
          "mqttUsername": {
            "type": "string"
          },
          "mqttPassword": {
            "type": "string"
          },
          "mqttCA": {
            "type": "string"
          },
          "hubKey": {
            "type": "string"
          },
          "hubId": {
            "type": "string"
          }
        },
        "example": {
          "mqttUsername": "exampleUsername",
          "mqttPassword": "examplePassword",
          "mqttCA": "exampleCA",
          "hubKey": "exampleHubKey",
          "hubId": "exampleHubId"
        }
      },

      "ManagerTeamRequest": {
        "type": "object",
        "properties": {
          "managerEmail": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          }
        },
        "example": {
          "managerEmail": "example@example.com",
          "teamId": "teamId"
        }
      },

      "PlayerTeamRequest": {
        "type": "object",
        "properties": {
          "playerEmail": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          }
        },
        "example": {
          "playerEmail": "example@example.com",
          "teamId": "teamId"
        }
      },

      "AnalyticsSummary": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "value": {
            "type": "string"
          },
          "trend": {
            "type": "number"
          },
          "metaUnits": {
            "type": "string"
          }
        },
        "example": [
          {
            "title": "Cumulative Impacts",
            "value": "850 g",
            "trend": -20
          },
          {
            "title": "Impacts Recorded",
            "value": "50",
            "trend": 20
          },
          {
            "title": "Impact Frequency",
            "value": "400",
            "metaUnits": "/hr",
            "trend": 10
          },
          {
            "title": "Average Impact",
            "value": "43 g",
            "trend": 40
          }
        ]
      },
      "PlayerRemoveRequest": {
        "type": "object",
        "properties": {
          "jersyId": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          }
        },
        "required": ["jersyId", "teamId"],
        "example": {
          "jersyId": "exampleJersyId",
          "teamId": "exampleTeamId"
        }
      },
  
      "PlayerRemoveResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "example": {
          "message": "Player removed successfully"
        }
      },
      "PlayerCreateRequest": {
        "type": "object",
        "properties": {
          "jersyId": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "teamId": {
